<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_profil__gui"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="uk.ac.leonarduea.haig.View.Profil_GUI"
    android:background="#FFFFFF">

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/location4"
        android:layout_below="@+id/location6"
        android:layout_alignStart="@+id/textView8"
        android:layout_marginTop="13dp"
        android:textColor="@color/colorPrimaryDark" />

    <TextView
        android:text="Name:"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView7"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_marginStart="15dp"
        android:layout_marginTop="14dp"
        android:textColor="@color/colorPrimaryDark" />

    <TextView
        android:text="Degree Progress"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView8"
        android:layout_marginTop="33dp"
        android:layout_below="@+id/userName"
        android:layout_alignStart="@+id/textView7"
        android:textColor="@color/colorPrimaryDark" />

    <TextView
        android:text="percent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/degreePercent"
        android:layout_alignBaseline="@+id/textView8"
        android:layout_alignBottom="@+id/textView8"
        android:layout_alignStart="@+id/degreeClass"
        android:textColor="@color/colorPrimaryDark" />

    <TextView
        android:text="Name"
        android:layout_width="200dp"
        android:layout_height="20dp"
        android:id="@+id/userName"
        android:layout_alignBaseline="@+id/textView7"
        android:layout_alignBottom="@+id/textView7"
        android:layout_alignStart="@+id/degreePercent"
        android:textColor="@color/colorPrimaryDark" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/location5"
        android:layout_marginTop="12dp"
        android:layout_below="@+id/location4"
        android:layout_alignStart="@+id/location4"
        android:textColor="@color/colorPrimaryDark" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/location3"
        android:layout_marginTop="13dp"
        android:layout_below="@+id/location5"
        android:layout_alignStart="@+id/location5"
        android:textColor="@color/colorPrimaryDark" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/location2"
        android:layout_marginTop="13dp"
        android:layout_below="@+id/location1"
        android:layout_alignStart="@+id/location1"
        android:textColor="@color/colorPrimaryDark" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/location1"
        android:layout_marginTop="14dp"
        android:layout_below="@+id/location3"
        android:layout_alignStart="@+id/location3"
        android:textColor="@color/colorPrimaryDark" />

    <TextView
        android:text="Locations Found:"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:id="@+id/textView21"
        android:textColor="@color/colorPrimaryDark"
        android:layout_below="@+id/progressBar2"
        android:layout_alignEnd="@+id/textView8" />

    <TextView
        android:text="class"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/degreeClass"
        android:layout_marginTop="39dp"
        android:textColor="@color/colorPrimaryDark"
        android:layout_below="@+id/degreePercent"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="65dp" />

    <TextView
        android:text="Degree Classification:"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView10"
        android:textColor="@color/colorPrimaryDark"
        android:layout_above="@+id/progressBar2"
        android:layout_alignStart="@+id/textView8" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/location6"
        android:layout_marginTop="15dp"
        android:textColor="@color/colorPrimaryDark"
        android:layout_below="@+id/textView21"
        android:layout_alignStart="@+id/location4" />

    <Button
        android:text="Graduate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/grad"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:background="@drawable/button"
        android:textColor="@color/colorPrimaryDark"
        android:layout_toEndOf="@+id/leader" />

    <ProgressBar
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="37dp"
        android:id="@+id/progressBar2"
        android:labelFor="@id/degreePercent"
        android:layout_below="@+id/degreeClass"
        android:layout_alignStart="@+id/textView21" />

    <Button
        android:text="Back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/back"
        android:background="@drawable/button"
        android:textColor="@color/colorPrimaryDark"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_toStartOf="@+id/leader" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/location6"
        android:layout_toStartOf="@+id/degreeClass"
        android:id="@+id/location7"
        android:textColor="@color/colorPrimaryDark" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/location5"
        android:layout_alignStart="@+id/location7"
        android:id="@+id/location8"
        android:textColor="@color/colorPrimaryDark" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/location5"
        android:layout_alignEnd="@+id/location8"
        android:id="@+id/location9"
        android:textColor="@color/colorPrimaryDark" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/location3"
        android:layout_alignStart="@+id/location9"
        android:id="@+id/location10"
        android:textColor="@color/colorPrimaryDark" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/location1"
        android:layout_alignStart="@+id/location10"
        android:id="@+id/location11"
        android:textColor="@color/colorPrimaryDark" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/location2"
        android:layout_alignStart="@+id/location11"
        android:id="@+id/location12"
        android:textColor="@color/colorPrimaryDark" />

    <Button
        android:text="LeaderBoard"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/grad"
        android:id="@+id/leader"
        android:background="@drawable/button"
        android:textColor="@color/colorPrimaryDark"
        android:layout_toEndOf="@+id/textView21" />

</RelativeLayout>
